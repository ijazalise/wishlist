<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
<acl>
    <resources>
        <resource id="Magento_Backend::admin">
            <resource id="Magento_Backend::marketing">
                <resource id="Magento_Backend::marketing_user_content">
                    <resource id="Innovadeltech_Wishlist::wishlist_notification" title="Wishlist Notification" sortOrder="10" >
                        <resource id="Innovadeltech_Wishlist::index" title="Show Grid Wishlist Notification" sortOrder="10" />
                        <resource id="Innovadeltech_Wishlist::edit" title="Edit Wishlist Notification" sortOrder="20" />
                        <resource id="Innovadeltech_Wishlist::save" title="Save Wishlist Notification" sortOrder="30" />
                        <resource id="Innovadeltech_Wishlist::delete" title="Delete Wishlist Notification" sortOrder="40" />
                    </resource>
                </resource>
            </resource>
        </resource>
    </resources>
</acl>
</config>